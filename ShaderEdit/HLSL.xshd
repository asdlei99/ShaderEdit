    <!-- This is the main ruleset. -->
    <RuleSet>
        <Import ruleSet="CommonSet"/>
        <Import ruleSet="NonAssemblerSet"/>
        
        <Span ruleSet="TechniquesAndPassesSet" multiline="true">
            <Begin color="Technique">
                \b(technique|Technique|technique10)\b
            </Begin>
            <End>}</End>
        </Span>
        
        <Span ruleSet="TechniquesAndPassesSet" multiline="true">
            <Begin color="StateBlock">
                \b(sampler_state|SamplerState|SamplerComparisonState|stateblock_state|BlendState|DepthStencilState|RasterizerState)\b
            </Begin>
            <End>}</End>
        </Span>
        
        <Span ruleSet="AssemblerSet" multiline="true">
            <Begin color="Assembler">\basm\b</Begin>
            <End>}</End>
        </Span>
        
        <Rule color="Semantics">
            (?&lt;=:\s*)          # : followed by whitespace
            (SV_[\dA-Za-z_]+      # a system value semantic (SV_Position, SV_ClipDistance0, ...)
            |[A-Z_]+[\dA-Z_]*)    # an upper-case semantic (POSITION, TEXCOORD0, ...)
        </Rule>
        
        <Keywords color="StorageClass">
            <Word>const</Word>
            <Word>extern</Word>
            <Word>nointerpolation</Word>
            <Word>register</Word>
            <Word>shared</Word>
            <Word>static</Word>
            <Word>uniform</Word>
            <Word>volatile</Word>
        </Keywords>
        
        <Keywords color="InterpolationModifiers">
            <Word>_linear</Word>
            <Word>_centroid</Word>
            <Word>_nointerpolation</Word>
            <Word>_noperspective</Word>
        </Keywords>
        
        <Keywords color="TypeModifiers">
            <Word>row_major</Word>
            <Word>column_major</Word>
            <Word>snorm</Word>
